@using ExcelQrProject.UI.Dtos;
@model List<EmployeeInformationsGetDto>

<table class="table table-bordered">
    <tr>
        <th>QR Kod</th>
        <th>Ad</th>
        <th>Soyad</th>
        <th>Phone</th>
        <th>Email</th>
        <th>Department</th>
        <th>Position</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Surname</td>
            <td>@item.Phone</td>
            <td>@item.Email</td>
            <td>@item.Department</td>
            <td>@item.Position</td>
        </tr>
        <tr>
            <td colspan="2"></td>
        </tr>
    }
</table>
